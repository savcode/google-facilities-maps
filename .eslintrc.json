{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "lines-between-class-members": "off"
    },
    "parser": "babel-eslint",
    "plugins": [
        "babel",
        "jest"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "globals": {
        "google": "readonly",
        "MarkerClusterer": "readonly"
    }
}